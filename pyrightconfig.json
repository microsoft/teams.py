{
   "include":[
      "packages/**/src/**/*.py",
      "tests/**/src/**/*.py"
   ],
   "venv": ".venv",
   "venvPath": ".",
   "extraPaths": [
      "packages/api/src",
      "packages/common/src",
      "packages/cards/src",
      "packages/apps/src",
      "packages/graph/src",
      "packages/devtools/src",
      "packages/ai/src",
      "packages/openai/src",
      "packages/mcpplugin/src"
   ],
   "typeCheckingMode": "strict",
   "executionEnvironments": [
      {
         "root": "packages/api/src",
         "reportIncompatibleVariableOverride": "none",
         "reportIncompatibleMethodOverride": "none"
      }
   ]
}
