{
   "include": [
      "packages/**/src/**/*.py"
   ],
   "venv": ".venv",
   "venvPath": ".",
   "extraPaths": [
      "packages/api/src",
      "packages/common/src",
      "packages/cards/src",
      "packages/app/src",
      "packages/devtools/src"
   ],
   "typeCheckingMode": "strict",
   "executionEnvironments": [
      {
         "root": "packages/api/src",
         "reportIncompatibleVariableOverride": "none",
         "reportIncompatibleMethodOverride": "none"
      }
   ]
}